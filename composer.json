{
    "name": "diego1araujo/titleasy",
    "description": "Build titles like a charm in Laravel 5",
    "keywords": ["laravel", "title"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Diego Araujo",
            "email": "diego77araujo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "~5"
    },
    "autoload": {
        "psr-4": {
            "Diego1araujo\\Titleasy\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Diego1araujo\\Titleasy\\ServiceProvider"
            ],
            "aliases": {
                "Title": "Diego1araujo\\Titleasy\\Facade"
            }
        }
    }
}
